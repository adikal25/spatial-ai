# API Keys
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI Configuration
OPENAI_VLM_MODEL=gpt-5-nano
OPENAI_TEMPERATURE=0.2
OPENAI_MAX_OUTPUT_TOKENS=2048

#(venv) python -m uvicorn src.api.main:app --reload

# Depth Model Configuration
DEPTH_MODEL=midas_v3_small
# Options: midas_v3_small, midas_v3_dpt_large, zoedepth

# Performance Settings
MAX_IMAGE_SIZE=1024
ENABLE_GPU=true
BATCH_SIZE=1

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
RELOAD=false

# Logging
LOG_LEVEL=INFO

# fVDB 3D Verification
ENABLE_FVDB_3D=true
FVDB_VOXEL_SIZE=0.05
FVDB_DEVICE=auto
